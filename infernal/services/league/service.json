
{
    "__metadata__": {
        "service_key": "league",
        "service_endpoint": "league",
        "service_version": "v4"
    },
    "requests": {
        "challenger_leagues_by_queue": {
            "method": "GET",
            "url": "challengerleagues/by-queue/${queue}",
            "path_params": {
                "queue": {
                    "dtype": "string",
                    "values": [
                        "RANKED_SOLO_5x5",
                        "RANKED_FLEX_SR",
                        "RANKED_FLEX_TT"
                    ],
                    "required": true
                }
            },
            "models": {
                "output": "league_list_dto"
            }
        },
        "entries_by_summoner_id": {
            "method": "GET",
            "url": "entries/by-summoner/${summoner_id}",
            "path_params": {
                "summoner_id": {
                    "key": "encryptedSummonerId",
                    "dtype": "string",
                    "required": true,
                    "encrypted": true,
                    "description": "Summoner ID associated with the player"
                }
            },
            "models": {
                "output": "set[league_entry_dto]"
            }
        },
        "entries_by_queue_tier_division": {
            "method": "GET",
            "url": "entries/${queue}/${tier}/${division}",
            "path_params": {
                "queue": {
                    "dtype": "string",
                    "values": [
                        "RANKED_SOLO_5x5",
                        "RANKED_FLEX_SR",
                        "RANKED_FLEX_TT"
                    ],
                    "required": true
                },
                "tier": {
                    "dtype": "string",
                    "values": [
                        "DIAMOND",
                        "PLATINUM",
                        "GOLD",
                        "SILVER",
                        "BRONZE",
                        "IRON"
                    ],
                    "required": true
                },
                "division": {
                    "dtype": "string",
                    "values": [
                        "I",
                        "II",
                        "III",
                        "IV"
                    ],
                    "required": true
                }
            },
            "models": {
                "output": "set[league_entry_dto]"
            }
        },
        "grandmaster_leagues_by_queue": {
            "method": "GET",
            "url": "grandmasterleagues/by-queue/${queue}",
            "path_params": {
                "queue": {
                    "dtype": "string",
                    "values": [
                        "RANKED_SOLO_5x5",
                        "RANKED_FLEX_SR",
                        "RANKED_FLEX_TT"
                    ],
                    "required": true
                }
            },
            "models": {
                "output": "league_list_dto"
            }
        },
        "leagues_by_id": {
            "method": "GET",
            "url": "leagues/${league_id}",
            "path_params": {
                "league_id": {
                    "key": "leagueId",
                    "dtype": "string",
                    "required": true,
                    "description": "The UUID of the league."
                }
            },
            "models": {
                "output": "league_list_dto"
            }
        },
        "master_leagues_by_queue": {
            "method": "GET",
            "url": "masterleagues/by-queue/${queue}",
            "path_params": {
                "queue": {
                    "dtype": "string",
                    "values": [
                        "RANKED_SOLO_5x5",
                        "RANKED_FLEX_SR",
                        "RANKED_FLEX_TT"
                    ],
                    "required": true
                }
            },
            "models": {
                "output": "league_list_dto"
            }
        }
    },
    "models": {
        "league_list_dto": {
            "__metadata__": {
                "class": "LeagueListDTO"
            },
