{
    "__metadata__": {
        "service_key": "mastery",
        "service_endpoint": "champion-mastery",
        "service_version": "v4"
    },
    "requests": {
        "mastery_by_summoner_id": {
            "method": "GET",
            "url": "champion-masteries/by-summoner/${summoner_id}",
            "path_params": {
                "summoner_id": {
                    "key": "encryptedSummonerId",
                    "dtype": "string",
                    "required": true,
                    "encrypted": true,
                    "description": "Summoner ID associated with the player"
                }
            },
            "models": {
                "output": "champion_mastery_dto"
            }
        },
        "mastery_by_summoner_champion_id": {
            "method": "GET",
            "url": "champion-masteries/by-summoner/${summoner_id}/by-champion/${champion_id}",
            "path_params": {
                "summoner_id": {
                    "key": "encryptedSummonerId",
                    "dtype": "string",
                    "required": true,
                    "encrypted": true,
                    "description": "Summoner ID associated with the player"
                },
                "champion_id": {
                    "key": "championId",
                    "dtype": "long",
                    "required": true,
                    "description": "Champion ID to retrieve the Champion Master for"
                }
            },
            "models": {
                "output": "champion_mastery_dto"
            }
        },
        "mastery_score_by_summoner_id": {
            "method": "GET",
            "url": "scores/by-summoner/${summoner_id}",
            "path_params": {
                "summoner_id": {
                    "key": "encryptedSummonerId",
                    "dtype": "string",
                    "required": true,
                    "encrypted": true,
                    "description": "Summoner ID associated with the player"
                }
            },
            "models": {
                "output": "int"
            }
        }
    },
    "models": {
        "champion_mastery_dto": {
            "__metadata__": {
                "class": "ChampionMasteryDTO",
                "description": "The object contains single Champion Mastery information for player and champion combination."
            },
            "chest_granted": {
                "key": "chestGranted",
                "dtype": "boolean",
                "description": "Is chest granted for this champion or not in current season."
            },
            "champion_level": {
                "key": "championLevel",
                "dtype": "int",
                "description": "Champion level for specified player and champion combination."
            },
            "champion_points": {
                "key": "championPoints",
                "dtype": "int",
                "description": "Total number of champion points for this player and champion combination- they are used to determine championLevel."
            },
            "champion_id": {
                "key": "championId",
                "dtype": "long",
                "description": "Champion ID for this entry."
            },
            "points_to_level": {
                "key": "championPointsUntilNextLevel",
                "dtype": "long",
                "description": "Number of poitns needed to achieve next level. Zero if player reached maximum champion level for this champion."
            },
            "last_play_time": {
                "key": "lastPlayTime",
                "dtype": "long",
                "converter": "unix_milliseconds",
                "description": "Last time this champion was played by this player - in Unix milliseconds time format."
            },
            "tokens_earned": {
                "key": "tokensEarned",
                "dtype": "int",
                "description": "The token earned for this champion to levelup."
            },
            "points_since_level": {
                "key": "champion PointsSinceLastLevel",
                "dtype": "long",
                "description": "Number of points earned since current level has been achieved."
            },
            "summoner_id": {
                "key": "summonerId",
                "dtype": "string",
                "encrypted": true,
                "description": "Summoner ID for this entry"
            }
        }
    }
}